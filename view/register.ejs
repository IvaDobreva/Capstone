<html lang="en" xmlns="http://www.w3.org/1999/html">
<head>
    <meta charset="UTF-8">
    <title>register</title>
    <link rel="stylesheet" type="text/css" href="/stylesheet/register.css">
</head>

<body>
<div id="register_frame">
    <p><h1> register </h1></p>
    <form method="post" action="/register">
        <p>
            <label class="label_register">username</label>
        <input type="text" id="username" class="text_register" />
        </p>
        <p>
            <label class="label_register">password</label>
        <input type="password" id="pass" class="text_register" />
        </p>
        <p>
            <label class="label_register">country</label>
        <input type="text" id="country" class="text_register" />
        </p>
        <p>
            <label class="label_register">e-mail</label>
        <input type="email" id="email" class="text_register" />
        </p>

    <div id="register_control">
        <p>
        <input type="submit" id="wt_register" value="register">
        <input type="reset" id="cancel" value="cancel">
        </p>
    </div>
    </form>
</div>
</body>
</html>